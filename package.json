{
    "name": "@disqada/pathfinder",
    "version": "1.1.1",
    "description": "Easily find files in chosen directories in your project",
    "scripts": {
        "build": "tsc",
        "test": "jest",
        "doc": "jsdoc -c jsdoc.json",
        "format": "prettier . --write",
        "lint": "eslint . --fix",
        "commit": "git-cz",
        "semantic-release": "semantic-release"
    },
    "main": "./src/exports.js",
    "module": "./src/exports.js",
    "types": "./types/exports.d.ts",
    "exports": {
        ".": {
            "node": "./src/exports.js",
            "require": "./src/exports.js",
            "import": "./src/exports.js",
            "types": "./types/exports.d.ts",
            "default": "./src/exports.js"
        }
    },
    "files": [
        "src/",
        "types/",
        "CHANGELOG.md"
    ],
    "homepage": "https://github.com/DisQada/pathfinder",
    "repository": {
        "type": "git",
        "url": "https://github.com/DisQada/pathfinder.git"
    },
    "bugs": {
        "url": "https://github.com/DisQada/pathfinder/issues"
    },
    "keywords": [
        "find",
        "search",
        "path",
        "paths",
        "directory",
        "folder",
        "file",
        "finder"
    ],
    "author": {
        "name": "The Alpha",
        "email": "contact@nabilalsaiad.com",
        "url": "https://links.nabilalsaiad.com"
    },
    "contributors": [],
    "license": "Apache-2.0",
    "devDependencies": {
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@types/node": "^18.15.11",
        "commitizen": "^4.3.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.39.0",
        "eslint-plugin-jest": "^27.2.3",
        "eslint-plugin-jsdoc": "^46.5.0",
        "jest": "^29.6.2",
        "jsdoc": "^4.0.2",
        "prettier": "^2.8.8",
        "semantic-release": "^21.0.1"
    },
    "publishConfig": {
        "access": "public"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
